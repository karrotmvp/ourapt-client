{
  "name": "ourapt-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "codegen:alpha": "openapi-generator-cli generate -i http://api-alpha.daangn-ourapt.com/v2/api-docs -g typescript-fetch -o ./src/__generated__/ourapt",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "msw": "dotenv -e .env.msw react-scripts start",
    "alpha": "dotenv -e .env.alpha react-scripts start",
    "alpha:build": "dotenv -e .env.alpha react-scripts build",
    "production": "dotenv -e .env.production react-scripts start",
    "production:build": "dotenv -e .env.production react-scripts build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@karrotframe/navigator": "^0.17.3",
    "@karrotmarket/mini": "^0.12.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.33",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "dotenv-cli": "^4.0.0",
    "msw": "^0.35.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
